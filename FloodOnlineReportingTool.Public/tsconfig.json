{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "bun-types"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "bun.build.ts"
  ]
}